# Unigram
U00:%x[-2,0]
U01:%x[-1,0]
U02:%x[0,0]
U03:%x[1,0]
U04:%x[2,0]

U05:%x[-2,1]
U06:%x[-1,1]
U07:%x[0,1]
U08:%x[1,1]
U09:%x[2,1]

U10:%x[-2,2]
U11:%x[-1,2]
U12:%x[0,2]
U13:%x[1,2]
U14:%x[2,2]

U15:%x[-1,0]/%x[0,0]
U16:%x[0,0]/%x[1,0]

U17:%x[-2,1]/%x[-1,1]
U18:%x[-1,1]/%x[0,1]
U19:%x[0,1]/%x[1,1]
U20:%x[1,1]/%x[2,1]

U21:%x[-2,2]/%x[-1,2]
U22:%x[-1,2]/%x[0,2]
U23:%x[0,2]/%x[1,2]
U24:%x[1,2]/%x[2,2]

U25:%x[-2,3]/%x[-1,3]/%x[0,3]
U26:%x[-1,3]/%x[0,3]/%x[1,3]
U27:%x[0,3]/%x[1,3]/%x[2,3]

U28:%x[-2,3]
U29:%x[-1,3]
U30:%x[0,3]
U31:%x[1,3]
U32:%x[2,3]

U33:%x[-2,3]/%x[-1,3]
U34:%x[-1,3]/%x[0,3]
U35:%x[0,3]/%x[1,3]
U36:%x[1,3]/%x[2,3]

# Bigram
B00:%x[-2,0]
B01:%x[-1,0]
B02:%x[0,0]
B03:%x[1,0]
B04:%x[2,0]

B05:%x[-2,1]
B06:%x[-1,1]
B07:%x[0,1]
B08:%x[1,1]
B09:%x[2,1]

B10:%x[-2,2]
B11:%x[-1,2]
B12:%x[0,2]
B13:%x[1,2]
B14:%x[2,2]

B15:%x[-1,0]/%x[0,0]
B16:%x[0,0]/%x[1,0]

B17:%x[-2,1]/%x[-1,1]
B18:%x[-1,1]/%x[0,1]
B19:%x[0,1]/%x[1,1]
B20:%x[1,1]/%x[2,1]

B21:%x[-2,2]/%x[-1,2]
B22:%x[-1,2]/%x[0,2]
B23:%x[0,2]/%x[1,2]
B24:%x[1,2]/%x[2,2]

B25:%x[-2,3]/%x[-1,3]/%x[0,3]
B26:%x[-1,3]/%x[0,3]/%x[1,3]
B27:%x[0,3]/%x[1,3]/%x[2,3]

B28:%x[-2,3]
B29:%x[-1,3]
B30:%x[0,3]
B31:%x[1,3]
B32:%x[2,3]

B33:%x[-2,3]/%x[-1,3]
B34:%x[-1,3]/%x[0,3]
B35:%x[0,3]/%x[1,3]
B36:%x[1,3]/%x[2,3]